{
    "name": "contact-form",
    "packageManager": "yarn@4.6.0",
    "scripts": {
        "dev": "concurrently \"yarn less:watch\" \"yarn server\"",
        "server": "live-server ./",
        "less:watch": "chokidar 'src/less/**/*.less' -c 'lessc src/less/index.less dist/index.css'"
    },
    "devDependencies": {
        "chokidar-cli": "^3.0.0",
        "concurrently": "^9.2.1",
        "less": "^4.4.2",
        "live-server": "^1.2.2"
    }
}
